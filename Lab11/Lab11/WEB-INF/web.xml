<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Lab11</display-name>
    <servlet>
        <servlet-name>Sss_header</servlet-name>
        <servlet-class>lab11.Sss_header</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_header</servlet-name>
        <url-pattern>/Sss_header</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_xml</servlet-name>
        <servlet-class>lab11.Sss_xml</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_xml</servlet-name>
        <url-pattern>/Sss_xml</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Sss_json</servlet-name>
        <servlet-class>lab11.Sss_json</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_json</servlet-name>
        <url-pattern>/Sss_json</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>final.html</welcome-file>
        <!-- <welcome-file>sss_header.html</welcome-file> -->
        <!-- <welcome-file>sss_xml.html</welcome-file> -->
        <!-- <welcome-file>sss_json.html</welcome-file> -->
    </welcome-file-list>
</web-app>